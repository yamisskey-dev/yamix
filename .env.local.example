# Local development secrets
# Copy this file to .env.local and fill in your actual values
# Infrastructure settings (DATABASE_URL, REDIS_URL, etc.) are set in .devcontainer/compose.yml

# OpenAI API Key (required for AI counseling)
OPENAI_API_KEY=your-openai-api-key-here

# JWT Secret (for authentication)
JWT_SECRET=yamix-dev-secret-key

# Yamii API Key (shared secret between Yamix and Yamii)
YAMII_API_KEY=dev-only-yamii-api-key-do-not-use-in-production
