# ===========================================
# Yamix ローカル開発用環境変数
# ===========================================
# Copy this file to .env.local for development without Docker
# cp .env.local.example .env.local

DATABASE_URL="postgresql://yamix:password@localhost:5432/yamix"
REDIS_URL="redis://localhost:6379"
JWT_SECRET=yamix-dev-secret-key
WEB_URL=http://localhost:3000
YAMII_API_URL=http://localhost:8000
# Yamii API Key (must match YAMII_API_KEYS in yamii/.env)
YAMII_API_KEY=dev-only-yamii-api-key-do-not-use-in-production
